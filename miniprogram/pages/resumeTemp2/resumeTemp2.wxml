<view class="page">
  <view class="resume-header">
    <view class="userInfo">
      <image class="photo" src="{{info.avatarUrl}}" />
      <view>
        <view class="userName">{{detail.name||'暂无数据'}}</view>
        <view class="expectJob">投递岗位：{{detail.expectjob||'暂无数据'}}</view>
      </view>
    </view>
    <view class="basicInfo">
      <view class="infoItem">
        <image class="icon" src="../../images/age1.svg" />
        <text class="message">年龄：{{detail.age||'暂无数据'}}岁</text>
      </view>
      <view class="infoItem">
        <image class="icon" src="../../images/wechat1.svg" />
        <text class="message">微信：{{detail.wechat||'暂无数据'}}</text>
      </view>
      <view class="infoItem">
        <image class="icon" src="../../images/phone1.svg" />
        <text class="message">手机：{{detail.phone||'暂无数据'}}</text>
      </view>
      <view class="infoItem">
        <image class="icon" src="../../images/email1.svg" />
        <text class="message">邮箱：{{detail.email||'暂无数据'}}</text>
      </view>
    </view>
  </view>
  <view class="body">
    <view class="resume-item">
      <view class="infoType">
        <text class="infoTitle">个人信息</text>
      </view>
      <view class="type">
        <view class="infoDesc">
          <image class="apeIcon" src="../../images/github.svg" />
          <view class="apeDesc">
            <text class="apeInfo">Stars {{searchData.Github['收藏'] || '暂无数据'}}</text>
          </view>
          <view class="apeDesc">
            <text class="apeInfo">Forks {{searchData.Github['叉子'] || '暂无数据'}}</text>
          </view>
          <view class="apeDesc">
            <text class="apeInfo">Followers {{searchData.Github['关注'] || '暂无数据'}}</text>
          </view>
        </view>
        <view class="infoDesc">
          <image class="apeIcon" src="../../images/LeetCode.svg" />
          <view class="apeDesc">
            <text class="apeInfo">竞赛排名 {{searchData['Leetcode']['竞赛排名'] || '暂无数据'}}</text>
          </view>
          <view class="apeDesc">
            <text class="apeInfo">刷题数 {{searchData['Leetcode']['刷题数'] || '暂无数据'}}</text>
          </view>
          <view class="apeDesc">
            <text class="apeInfo">竞赛积分 {{searchData['Leetcode']['竞赛积分'] || '暂无数据'}}</text>
          </view>
        </view>
        <view class="infoDesc">
          <image class="apeIcon" src="../../images/juejin.svg" />
          <view class="apeDesc">
            <text class="apeInfo">掘力值    {{searchData['掘金']['掘力值'] || '暂无数据'}}</text>
            <text class="apeInfo">关注者   {{searchData['掘金']['关注者'] || '暂无数据'}}</text>
          </view>
          <view class="apeDesc">
            <text class="apeInfo">发布文章  {{searchData['掘金']['文章数'] || '暂无数据'}}</text>
            <text class="apeInfo">获得点赞  {{searchData['掘金']['点赞'] || '暂无数据'}}</text>
          </view>
          <view class="apeDesc">
            <text class="apeInfo">文章被阅读  {{searchData['掘金']['文章被观看'] || '暂无数据'}}</text>
          </view>
        </view>
      </view>
    </view>
    <view class="resume-item">
      <view class="infoType">
        <text class="infoTitle">工作经验</text>
      </view>
      <view class="type">
        <view class="infoDesc" wx:for="{{selected.works}}" wx:key="item">
          <view class="time">
            <text class="time">{{item.startDate||'暂无数据'}}</text>
            <text class="time">~</text>
            <text class="time">{{item.endDate||'暂无数据'}}</text>
          </view>
          <view class="desc">
            <view class="flex">
              <view class="descName">{{item.company||'暂无数据'}}</view>
              <view class="descJob">{{item.job||'暂无数据'}}</view>
            </view>
            <view class="descInfo">{{item.desc||'暂无数据'}}</view>
          </view>
        </view>
      </view>
    </view>
    <view class="resume-item">
      <view class="infoType">
        <text class="infoTitle">项目成果</text>
      </view>
      <view class="type">
        <view wx:for="{{selected.items}}" wx:key="item">
          <view class="infoDesc">
            <view>
              <image class="itemImg" src="../../images/ape.png" />
              <text class="time">{{item.startDate||'暂无数据'}}</text>
              <text class="time">~</text>
              <text class="time">{{item.endDate||'暂无数据'}}</text>
            </view>
            <view class="desc">
              <view class="descName">{{item.proname||'暂无数据'}}</view>
              <view class="descJob">技术栈：{{item.techstack||'暂无数据'}}</view>
              <view class="descInfo">{{item.desc||'暂无数据'}}</view>
            </view>
          </view>
        </view>
      </view>
    </view>
    <view class="resume-item">
      <view class="infoType">
        <text class="infoTitle">教育背景</text>
      </view>
      <view class="type">
        <view class="infoDesc">
          <view class="time">
            <text class="time">{{detail.startDate||'暂无数据'}}</text>
            <text class="time">~</text>
            <text class="time">{{detail.endDate||'暂无数据'}}</text>
          </view>
          <view class="desc">
            <view class="flex">
              <view class="descName">{{detail.schoolname||'暂无数据'}}</view>
              <view class="descJob">{{detail.major||'暂无数据'}}（{{detail.education||'暂无数据'}}）</view>
            </view>
            <view class="descInfo">个人优势：{{detail.advantage||'暂无数据'}}</view>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>